RewriteRule ^order-success order-success.php
RewriteRule ^cam-nang-mua-hang cam-nang.php
RewriteRule ^doi-tra-hang chinh-sach-doi-tra.php

RewriteRule ^([a-zA-Z0-9_-]+)$ detail.php?id=$1
